{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jasmine": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "import/newline-after-import": [
          "off"
        ],
        "import/no-extraneous-dependencies": [
          "off"
        ],
        "max-len": [
          "off"
        ],
        "no-underscore-dangle": [
          "off"
        ],
        "no-param-reassign": [
          "off"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "new-cap": [
          "off"
        ],
        "object-shorthand":[
          "off"
        ],
        "consistent-return": [
          "off"
        ],
        "array-callback-return": [
          "off"
        ],
        "brace-style": [
          "off"
        ],
        "eqeqeq": [
          "off"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
